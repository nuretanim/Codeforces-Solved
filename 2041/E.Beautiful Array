#include <bits/stdc++.h>
using namespace std;

int32_t main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);
  int a, b;
  cin >> a >> b;
  
  cout << "3\n" << b << " " << b << " " << 3 * a - 2 * b << "\n";

  return 0;
}
